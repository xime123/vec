<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vec.com.vec">

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/vec_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/vec_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".start.RegisterActivity" />
        <activity android:name=".start.RegistInfoActivity" />
        <activity android:name=".content.EyeTracking" />
        <activity
            android:name=".start.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".account.PickActivity" />
        <activity android:name=".mine.SecurityActivity" />
        <activity android:name=".start.PermissionActivity"></activity>
    </application>

</manifest>